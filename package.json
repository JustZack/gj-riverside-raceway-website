{
  "name": "gj-riverside-raceway-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:generate": "npx prisma generate",
    "prisma:push": "npx prisma db push",
    "prisma:seed": "npx prisma db seed",
    "prisma:init": "npm run prisma:generate && npm run prisma:push && npm run prisma:seed",
    "tools:scrape": "python ./tools/livetime_scraper/livetime.rc.scraper.py && mv seed.data.json prisma/seed.data.json",
    "scrape-and-init-db": "npm run tools:scrape && npm run prisma:init"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.10",
    "@fullcalendar/daygrid": "^6.1.10",
    "@fullcalendar/react": "^6.1.10",
    "@prisma/client": "^5.22.0",
    "@types/react-big-calendar": "^1.16.3",
    "date-fns": "^4.1.0",
    "next": "^15.1.0",
    "react": "^18.3.1",
    "react-big-calendar": "^1.19.4",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.23",
    "eslint": "^8",
    "eslint-config-next": "^15.1.0",
    "postcss": "^8.5.6",
    "prisma": "^5.22.0",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
